{
	"printWidth": 100,
	"tabWidth": 2,
	"useTabs": true,
	"semi": true,
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"singleQuote": false,
	"trailingComma": "es5",
	"endOfLine": "lf",
	"htmlWhitespaceSensitivity": "css",
	"jsxSingleQuote": false,
	"quoteProps": "as-needed",
	"requirePragma": false,
	"insertPragma": false,
	"proseWrap": "always"
}
